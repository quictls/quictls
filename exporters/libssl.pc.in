libdir=$(INSTALLPREFIX)/lib
includedir=$(INSTALLPREFIX)/include

Name: OpenSSL-libssl
Description: QUIC, TLS and cryptography libraries
Version: ${OPENSSL_FULL_VERSION_STRING}
